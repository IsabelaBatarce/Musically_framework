    <input class="flex-4" type="text" placeholder="Digite o RG" id="rg"/>
  </div>

  <div class="input-container">
    <label class="flex-1" for="email">Email</label>
    <input class="flex-9" type="text" placeholder="Digite seu email" id="email"/>
  </div>

  <div class="input-container">
    <label class="flex-1" for="senha">Senha</label>
    <input id="senha" class="flex-9" type="password" placeholder="Digite sua senha" />
  </div>

  <input id="btnAcessar" type="button"  value="Enviar" /> 

</form>
