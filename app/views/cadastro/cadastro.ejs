    <input class="flex-9" type="text" placeholder="Digite seu nome" id="nome"/>
  </div>
  
  <div class="input-container">
    <label class="flex-1" for="cpf">CPF</label>
    <input class="flex-4" type="text"  placeholder="Digite seu CPF" id="cpf"/>
